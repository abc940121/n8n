{
  "meta_info": {
    "version": "1.0.1",
    "description": "n8n Node Ontology generated from raw node definitions",
    "base_source": "merged_n8n_nodes.json"
  },
  "concepts": {
    "SystemNode": {
      "description": "代表通用系統操作或工作流節點的基礎實體 (Base Entity)。",
      "type": "Abstract",
      "properties": [
        {
          "name": "resource_entity",
          "description": "操作的資源對象 (如: User, Ticket)",
          "source_keys": ["resource", "table", "collection", "list"]
        },
        {
          "name": "operation_action",
          "description": "執行的動作 (如: Create, Get, Update)",
          "source_keys": ["operation", "action"]
        },
        {
          "name": "node_name",
          "source_keys": ["name", "displayName"]
        },
        {
          "name": "node_description",
          "source_keys": ["toolDescription", "description"]
        }
      ]
    },
    "IntegrationNode": {
      "inherits_from": "SystemNode",
      "description": "負責透過 API 或 HTTP 連接外部系統的實體。",
      "properties": [
        {
          "name": "connection_credential",
          "source_keys": ["authentication", "credentials"]
        },
        {
          "name": "external_entity_id",
          "description": "外部系統的唯一識別碼",
          "source_keys": ["id", "contactId", "userId", "taskId", "dealId", "fileId", "messageId", "channelId", "projectId"]
        },
        {
          "name": "api_url",
          "source_keys": ["url", "domain", "subdomain"]
        },
        {
          "name": "api_path",
          "source_keys": ["path"]
        }
      ],
      "components": {
        "pagination": {
          "description": "分頁控制邏輯",
          "properties": [
            {
              "name": "pagination_limit",
              "source_keys": ["limit", "maxResults"]
            },
            {
              "name": "pagination_mode",
              "source_keys": ["returnAll", "simple"]
            }
          ]
        },
        "data_control": {
          "description": "資料篩選與欄位控制",
          "properties": [
            {
              "name": "field_options",
              "source_keys": ["options", "jsonParameters"]
            },
            {
              "name": "field_updates",
              "source_keys": ["updateFields", "additionalFields"]
            },
            {
              "name": "data_filters",
              "source_keys": ["filters", "conditions"]
            }
          ]
        }
      }
    },
    "AIAgentNode": {
      "inherits_from": "SystemNode",
      "description": "AI 模型互動與 LLM 鏈的實體。",
      "properties": [
        {
          "name": "ai_model",
          "source_keys": ["model", "modelId", "modelName"]
        },
        {
          "name": "ai_prompt",
          "source_keys": ["prompt", "systemMessage", "userMessage"]
        },
        {
          "name": "ai_tool_definition",
          "source_keys": ["tool", "toolDescription", "toolName"]
        },
        {
          "name": "ai_embedding_settings",
          "source_keys": ["embeddingBatchSize", "embeddingDimension"]
        }
      ]
    },
    "MessagingNode": {
      "inherits_from": "SystemNode",
      "description": "處理訊息傳遞與通知的實體。",
      "properties": [
        {
          "name": "message_content",
          "source_keys": ["message", "text", "body", "content"]
        },
        {
          "name": "message_subject",
          "source_keys": ["subject", "title"]
        },
        {
          "name": "recipient_target",
          "source_keys": ["email", "channelId", "chatId", "to", "from"]
        }
      ]
    },
    "DataProcessingNode": {
      "inherits_from": "SystemNode",
      "description": "處理檔案與資料轉換的實體。",
      "properties": [
        {
          "name": "binary_property",
          "source_keys": ["binaryPropertyName"]
        },
        {
          "name": "data_format",
          "source_keys": ["format", "encoding"]
        },
        {
          "name": "file_details",
          "source_keys": ["fileId", "fileName", "mimeType"]
        }
      ]
    },
    "CRMEntity": {
      "description": "客戶關係管理 (CRM) 專用欄位。",
      "inherits_from": "IntegrationNode",
      "properties": [
        {
          "name": "contact_email",
          "source_keys": ["email", "contactEmail"]
        },
        {
          "name": "lead_status",
          "source_keys": ["status", "leadStatus", "stage"]
        },
        {
          "name": "crm_identifiers",
          "source_keys": ["contactId", "dealId", "companyId"]
        }
      ]
    },
    "TaskEntity": {
      "description": "任務與專案管理欄位。",
      "inherits_from": "IntegrationNode",
      "properties": [
        {
          "name": "task_identifiers",
          "source_keys": ["taskId", "cardId", "itemId"]
        },
        {
          "name": "project_identifiers",
          "source_keys": ["projectId", "boardId", "listId", "workspaceId"]
        },
        {
          "name": "task_due_date",
          "source_keys": ["due", "dueDate", "start", "end"]
        }
      ]
    }
  }
}