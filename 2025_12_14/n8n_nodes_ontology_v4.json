n8n_node_ontology = [
    # --- 1. 識別維度 (Identity) ---
    # 用於唯一識別與基礎描述，確保每個節點都有這些資料
    "source_filename",        # 來源檔案名稱 (e.g., "actionNetwork.json")
    "internal_id",            # 系統內部 ID (e.g., "n8n-nodes-base.actionNetwork") - 對應 name
    "display_name",           # UI 顯示名稱 (e.g., "Action Network")
    "description",            # 節點功能簡介
    "icon_path",              # 圖示路徑或名稱
    "documentation_url",      # 官方文件連結 (若 metadata 中有)
    
    # --- 2. 功能維度 (Capability) ---
    # 核心邏輯層，將 n8n 的 resource/operation 模式標準化
    "supported_resources",    # [List] 該節點支援的所有「資源」選項 (e.g., ["Attendance", "Event", "Person"])
    "supported_operations",   # [List] 該節點支援的所有「動作」選項 (e.g., ["create", "getAll", "update"])
    "auth_types",             # [List] 支援的認證方式 (e.g., ["oAuth2", "actionNetworkApi"])
    "is_trigger",             # [Boolean] 是否為觸發節點 (由名稱或屬性判斷)
    
    # --- 3. 參數維度 (Parameters) ---
    # 為了解決「欄位太多」的問題，將細節參數抽象化，方便後續搜尋或分群
    "required_parameter_keys",# [List] 必填的參數鍵名 (e.g., ["calendarId", "email"])
    "all_parameter_keys",     # [List] 所有出現過的參數鍵名 (用於計算相似度或覆蓋率)
    "default_resource",       # 預設選中的資源 (若有)
    "default_operation"       # 預設選中的動作 (若有)
]